package files;

public class PayLoadLibrary {
	
	public static String addBook(String isbn, String aisle) {
		
		return "{\r\n"
				+ "\r\n"
				+ "\"name\":\"Learn Appium Automation with Java Automation Test Practice 205\",\r\n"
				+ "\"isbn\":\""+isbn+"\",\r\n"
				+ "\"aisle\":\""+aisle+"\",\r\n"
				+ "\"author\":\"Karthik Test Automation Practice\"\r\n"
				+ "}\r\n"
				+ "";
		
	}

}
